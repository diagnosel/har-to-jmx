rentIndex);\n            currentIndex -= 1;\n\n            // And swap it with the current element.\n            temporaryValue = array[currentIndex];\n            array[currentIndex] = array[randomIndex];\n            array[randomIndex] = temporaryValue;\n        }\n\n        return array;\n    }\n\n    function check_cookie_name(name) \n    {\n      var match = document.cookie.match(new RegExp('(^| )' + name + '=([^;]+)'));\n      if (match) {\n        return match[2];\n      }\n    }\n\n    function getFirstLooks() {\n        if(looksIds.length === 0 && jQuery('.matching-component.container .cards-container .matching-card-container').length === 0) {\n            setNoMoreLooks();\n            dataLayer.push({'event': 'STinder_looks_limit'});\n            return;\n        }\n\n        var param = looksIds[0]\n        var url = `${baseUrlApi}looks?looks=${param}`;\n        jQuery.ajax({\n            url: url,\n            type: 'GET',\n            headers: header,\n            cache: false,\n            success: function(data) {\n                if (data.length > 0) {\n                    looks.push(data[0]);\n                } \n                looksIds.shift();\n                fillData(data);\n                loadedCards++;\n                if(loadedCards === 2) {\n                    loadedCards = 1;\n                    jQuery.each(jQuery('.matching-component.container .cards-container .matching-card-container'), function(index, value) {\n                        var currentElement = jQuery(value);\n                        currentElement.css({\n                            'z-index': looksIds.length - index\n                        });\n\n                        // if (index === 0) {\n                        //     currentElement.addClass('current-card');\n                        // }\n                    });                    \n                } else {\n                    getFirstLooks()\n                }\n\n                if (looksIds.length > 0 && !init) 