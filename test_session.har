type}_looks`);\n        var current = strCurrent ? JSON.parse(strCurrent) : [];\n        var exists = current.indexOf(id) > -1;\n\n        if(!exists) {\n            localStorage.setItem(\n                `${type}_looks`,\n                JSON.stringify([...current, id])\n            );\n        }\n\n        if(type === 'liked') {\n            var strCurrentWishlist = localStorage.getItem('wishlist');\n            var currentWishlist = strCurrentWishlist\n                ? JSON.parse(strCurrentWishlist)\n                : {};\n\n            currentWishlist[id] = {\n                ...look,\n                likedAt: Date.now()\n            };\n\n            localStorage.setItem('wishlist', JSON.stringify(currentWishlist));\n        }\n    }\n\n    function postLikeDislike(value) {\n        var firstLookContainer = jQuery('.matching-component.container .frame .cards-container > div > div.matching-card-container:first-of-type');\n        if(!firstLook) {\n            firstLook = jQuery('.matching-component.container .frame .cards-container > div > div.matching-card-container:first-of-type .matching-card > div');\n        }\n        if (firstLookContainer) {\n            var id = firstLookContainer.attr('data-lookId');\n            var matchingCard = jQuery(`.matching-card-container[data-lookid='${id}'] .matching-card`);\n            matchingCard.addClass(value === 1 ? 'loved' : 'passed');\n            var findLookObj = looks.find(x => x._id === id);\n            // set localstorage\n            localStorageSetup(id, findLookObj, value);\n\n            var params = {\n                storeUserId: userId,\n                isLike: value,\n                lookId: id,\n                offsetUtc: Date.now()\n            }\n            var url = `${baseUrlApi}users/likes`;\n\n            jQuery.ajax({\n                url: url,\n                type: 'POST',\n                data: JSON.stringify(params),\n                headers: header,\n                success: function (re