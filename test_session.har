(Math.round(number / 1000)) + 'K';\n            }\n            // millions\n            else if (number >= 1000000 && number <= 999999999) {\n                return (Math.round(number / 100000)/10) + 'M';\n            }\n            // billions\n            else if (number >= 1000000000 && number <= 999999999999) {\n                return (Math.round(number / 1000000000)) + 'B';\n            } else\n                return String(number);\n        }\n    }\n\n    function fillData(looks) {\n        var numberOfLooks = looks.length + 1;\n        for(var i = 0; i < looks.length; i++) {\n            var look = looks[i];\n            var template = jQuery('.matching-card-container.template-card-hidden').clone();\n            template.removeClass('template-card-hidden');\n            template.attr('data-lookId', look._id);\n\n            var watchTheTransformationLink = template.find('.image-watch-btn > a');\n            watchTheTransformationLink.attr('href', '/tutorial/' + look._id);\n\n            var imageContainer = template.find('.image-container');\n            imageContainer.attr('data-lookId', look._id);\n\n            // if image exit\n            var image = template.find('.image-container .image > img');\n            var type = undefined;\n            if(look.afterImage) {\n                type = 'afterImage';\n            } else if(look.bodyImage) {\n                type = 'bodyImage';\n            } else if(look.beforeImage) {\n                type = 'beforeImage';\n            }\n\n            if(type) {\n                image.attr('src', `${look[type]['mediumCachedUrl'] || look[type]['cachedUrlAk'] || look['specialImageUrl']}`)\n            } else {\n                template.find('.image-container .image').css({'display': 'none'});\n            }\n            // end if image exit\n\n            // influencer data\n            if(look.influencerRef &&\n               look.influencerRef.socialAccounts.instagram &&\n               look.influencerRef.socialAcc